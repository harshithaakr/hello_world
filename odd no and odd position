#include <stdio.h>

int main()
{
    int n,count=0;
    scanf("%d",&n);
    int a[n],b[n];
    for(int i=0;i<n;i++){
        scanf("%d",&a[i]);
    }
    
  
    for(int i=0;i<n;i++){
        if(i%2==0 && a[i]%2==1){
                b[count]=a[i];
                count++;
              
            }
        
        
    }
    for(int i=0;i<count;i++){
        printf("%d",b[i]);
    }
}
